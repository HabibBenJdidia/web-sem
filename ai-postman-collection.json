    {
      "name": "AI SPARQL - Get All Tourists",
      "request": {
        "method": "POST",
        "header": [{"key": "Content-Type", "value": "application/json"}],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"query\": \"PREFIX eco: <http://example.org/eco-tourism#>\\nSELECT ?tourist ?name ?age WHERE {\\n  ?tourist a eco:Touriste .\\n  ?tourist eco:nom ?name .\\n  OPTIONAL { ?tourist eco:age ?age }\\n}\\nLIMIT 10\"\n}"
        },
        "url": {
          "raw": "http://localhost:8000/ai/sparql",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["ai", "sparql"]
        },
        "description": "Execute SPARQL query with AI explanation"
      }
    },
