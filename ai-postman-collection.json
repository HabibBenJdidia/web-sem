        "body": {
          "mode": "raw",
          "raw": "{\n  \"query\": \"PREFIX eco: <http://example.org/eco-tourism#>\\nSELECT ?hebergement ?nom ?niveau WHERE {\\n  ?hebergement a eco:Hebergement .\\n  ?hebergement eco:nom ?nom .\\n  ?hebergement eco:niveauEco ?niveau .\\n  FILTER(?niveau = 'Gold' || ?niveau = 'Silver')\\n}\"\n}"
        },
        "url": {
          "raw": "http://localhost:8000/ai/sparql",
          "protocol": "http",
          "host": ["localhost"],
          "port": "8000",
          "path": ["ai", "sparql"]
        }
      }
